{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Run npm start 起動するだけ",
      "command": "npm run start"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost デバックモードにする",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true
    }
  ]
}
